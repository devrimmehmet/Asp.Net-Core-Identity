
@{
    ViewData["Title"] = "UserList";
}
@model List<UserViewModel>
<h1>Üye Listesi</h1>
<table class="table table-striped">
    <thead>
        <tr>
<th>Id</th>  
<th>UserName</th>
<th>Email</th>
</tr>
    </thead>
    <tbody>
         @foreach (var item in Model)
        {
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Email</td>
        }
    </tbody>

</table>
